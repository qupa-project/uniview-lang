fn CompareFloats() {
	let a = 3.0;
	let b = 7.0;

	let c = a == b;
	c = a != b;
	c = a > b;
	c = a < b;
	c = a >= b;
	c = a <= b;
	return;
}

fn CompareInts(): bool {
	let a = 3;
	let b = 7;

	let c = a == b;
	if (a == b) {
		return false;
	}
	if (4 < 3) {
		return false;
	}
	if (a > b) {
		return false;
	}
	if (4 >= 5) {
		return false;
	}
	if (5 <= 4) {
		return false;
	}
