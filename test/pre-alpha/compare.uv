}

fn CompareBools (): bool {
	let a = true;
	let b = false;

	let c = !a;
	c = a || b;
	c = a && b;

	if ((true || false) != true) {
		return false;
	}
	if (true && false != false) {
		return false;
	}
	if (true && true != true) {
		return false;
	}
	if (!true == true) {
		return false;
	}

	return true;
}

