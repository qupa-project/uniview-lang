class Cat {
	id: int;

	fn New(id: int): Cat {
		let out = Blank#[Cat]();
		out.id = id;
		return out;
	}

	fn Say(this: Cat) {
		printf("%i: meow\n", this.id);
		Cat.Delete(this);
	}

	fn Say(this: @Cat) {
		printf("%i: meow\n", this.id);
	}

	fn Clone(src: @Cat): Cat {
		let out = Blank#[Cat]();
		printf("%s", "Cloning cat\n");
		out.id = src.id + 1;

		return out;
	}

	fn Delete(this: Cat) {
		printf("%s", "Deleting cat...\n");
		delete this.id;
	}
}

fn main(): int {
	let c = Cat.New(2);
	Cat.Say($c);
	Cat.Say(@c);
	Cat.Delete(c);
	return 0;
}